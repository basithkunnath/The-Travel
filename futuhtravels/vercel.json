{
  "version": 2,
  "builds": [
    {
      "src": "futuhtravels/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",   
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",          
      "dest": "futuhtravels/wsgi.py"
    }
  ]
}
