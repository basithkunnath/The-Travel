{
  "builds": [
    {
      "src": "futuhtravels/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12",
        "buildCommand": "bash setup.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "futuhtravels/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ],
   "rewrites": [{ "source": "/(.*)", "destination": "/" }] 
}
